{
  "name": "qms-tradingdesk-client",
  "version": "1.0.0",
  "description": "QMS Trading desk client",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc mocha 'lib/**/*.spec.js'",
    "test": "npm run test:lib:fulltest && npm run test:react:fulltest",
    "build:clean": "./node_modules/.bin/rimraf ./public",
    "build:react:dev": "npm run build:clean  && ./node_modules/.bin/webpack --config webpack/webpack.dev.babel.js --color -p --progress --watch",
    "build:react:production": "npm run build:clean && ./node_modules/.bin/webpack --config webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "start:dev": "npm run build:react:dev & DEBUG=qms-tradingdesk-client:* ./node_modules/.bin/nodemon index.js ",
    "start:production": "node index.js",
    "test:lib:unit": "DEBUG=NONE ./node_modules/.bin/mocha 'lib/**/*.spec.js'",
    "test:lib:integration": "DEBUG=NONE ./node_modules/.bin/mocha --exit 'test/**/*.spec.js'",
    "test:lib:jslint": "./node_modules/.bin/standard 'lib/**/*.js'",
    "test:lib:fulltest": "npm run test:lib:jslint && npm run test:lib:unit && npm run test:lib:integration",
    "test:react:unit": "./node_modules/.bin/mocha --require @babel/register --require client/app/test/setup.test.js --require ignore-styles 'client/app/**/*.spec.js'",
    "test:react:jslint": "./node_modules/.bin/standard --parser babel-eslint 'client/app/**/*.js'",
    "test:react:fulltest": "npm run test:react:jslint && npm run test:react:unit"
  },
